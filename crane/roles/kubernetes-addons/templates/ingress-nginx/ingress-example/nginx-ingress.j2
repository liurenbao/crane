apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: example
  namespace: default
spec:
  rules:
  - host: {{ ingress_nginx_example }}
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: example-nginx
            port: 
              number: 80
